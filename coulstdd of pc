<p class=MsoNoSpacing><span style='font-family:"Segoe UI Symbol","sans-serif"'>&#128295;</span>
2. C++ Code Sample (launcher.cpp)</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>Here�s a foundational structure for your C++ launcher
app:</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>cpp</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>#include &lt;iostream&gt;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>#include &lt;cstdlib&gt;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>#include &lt;string&gt;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>// Replace with paths on your system</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>const std::string PYTHON_EXE = &quot;python&quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>const std::string SCRIPT_DIR =
&quot;C:\\ImagenAIGenerator\\scripts\\&quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>const std::string AHK_SCRIPT =
&quot;C:\\ImagenAIGenerator\\ahk\\instagram_automation.ahk&quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>void runCommand(const std::string&amp; command) {</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; int status = system(command.c_str());</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; if (status != 0) {</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; std::cerr
&lt;&lt; &quot;Command failed: &quot; &lt;&lt; command &lt;&lt; std::endl;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; }</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>}</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>void generateContent() {</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::string imagePath, prompt, phase;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::cout &lt;&lt; &quot;Enter image
path: &quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::getline(std::cin, imagePath);</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::cout &lt;&lt; &quot;Enter AI
prompt: &quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::getline(std::cin, prompt);</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::cout &lt;&lt; &quot;Enter brand
phase (e.g. Faza 0): &quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::getline(std::cin, phase);</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::string command = PYTHON_EXE +
&quot; \&quot;&quot; + SCRIPT_DIR + &quot;generate_content_helper.py\&quot;
\&quot;&quot; + imagePath + &quot;\&quot; \&quot;&quot; + prompt +
&quot;\&quot; \&quot;&quot; + phase + &quot;\&quot;&quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; runCommand(command);</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>}</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>void analyzeProduct() {</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::string imagePath, prompt;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::cout &lt;&lt; &quot;Enter image
path: &quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::getline(std::cin, imagePath);</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::cout &lt;&lt; &quot;Enter
optional prompt: &quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::getline(std::cin, prompt);</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::string command = PYTHON_EXE +
&quot; \&quot;&quot; + SCRIPT_DIR + &quot;analyze_product_helper.py\&quot;
\&quot;&quot; + imagePath + &quot;\&quot; \&quot;&quot; + prompt +
&quot;\&quot;&quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; runCommand(command);</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>}</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>void publishPost() {</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::string url, caption;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::cout &lt;&lt; &quot;Enter
image/video URL: &quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::getline(std::cin, url);</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::cout &lt;&lt; &quot;Enter post
caption: &quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::getline(std::cin, caption);</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::string command = PYTHON_EXE +
&quot; \&quot;&quot; + SCRIPT_DIR + &quot;publish_post_helper.py\&quot;
\&quot;&quot; + url + &quot;\&quot; \&quot;&quot; + caption +
&quot;\&quot;&quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; runCommand(command);</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>}</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>void getInsights() {</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::string mediaId, metrics;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::cout &lt;&lt; &quot;Enter media
ID (or leave blank for profile): &quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::getline(std::cin, mediaId);</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::cout &lt;&lt; &quot;Enter metrics
(e.g. impressions,reach,engagement): &quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::getline(std::cin, metrics);</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::string command = PYTHON_EXE +
&quot; \&quot;&quot; + SCRIPT_DIR + &quot;get_insights_helper.py\&quot;
\&quot;&quot; + mediaId + &quot;\&quot; \&quot;&quot; + metrics +
&quot;\&quot;&quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; runCommand(command);</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>}</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>void runAHKMenu() {</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; std::string command =
&quot;\&quot;&quot; + AHK_SCRIPT + &quot;\&quot;&quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; runCommand(command);</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>}</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>int main() {</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; int choice = -1;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; while (choice != 0) {</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; std::cout
&lt;&lt; &quot;\n--- Imagen AI Generator ---\n&quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; std::cout
&lt;&lt; &quot;1. Generate Instagram Content\n&quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; std::cout
&lt;&lt; &quot;2. Analyze Product Image\n&quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; std::cout
&lt;&lt; &quot;3. Publish Instagram Post\n&quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; std::cout
&lt;&lt; &quot;4. Get Instagram Insights\n&quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; std::cout
&lt;&lt; &quot;5. Launch AutoHotkey Menu\n&quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; std::cout
&lt;&lt; &quot;0. Exit\n&quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; std::cout
&lt;&lt; &quot;Choose option: &quot;;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;std::cin
&gt;&gt; choice;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
std::cin.ignore();&nbsp; // Clear newline</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; switch
(choice) {</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
case 1: generateContent(); break;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
case 2: analyzeProduct(); break;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
case 3: publishPost(); break;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
case 4: getInsights(); break;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
case 5: runAHKMenu(); break;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
case 0: break;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
default: std::cout &lt;&lt; &quot;Invalid choice.\n&quot;; break;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; }</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; }</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;&nbsp;&nbsp; return 0;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>}</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing><span style='font-family:"Segoe UI Symbol","sans-serif"'>&#128421;</span>&#65039;
Compile Instructions</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>If you're using Visual Studio:</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>Create a new Console App project</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>&nbsp;</p>

