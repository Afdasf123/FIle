<p class=MsoNoSpacing>bashrc.txt</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing># Load up standard site-wide settings.</p>

<p class=MsoNoSpacing>source /etc/bashrc</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>#remove duplicate entries from history</p>

<p class=MsoNoSpacing>export HISTCONTROL=ignoreboth</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing># Show current git branch in prompt.</p>

<p class=MsoNoSpacing>function parse_git_branch {</p>

<p class=MsoNoSpacing>� git branch --no-color 2&gt; /dev/null | sed -e
'/^[^*]/d' -e 's/* \(.*\)/(\1)/'</p>

<p class=MsoNoSpacing>}</p>

<p class=MsoNoSpacing>RED=&quot;\[\033[0;31m\]&quot;</p>

<p class=MsoNoSpacing>YELLOW=&quot;\[\033[0;33m\]&quot;</p>

<p class=MsoNoSpacing>GREEN=&quot;\[\033[0;32m\]&quot;</p>

<p class=MsoNoSpacing>LIGHT_GREEN=&quot;\[\033[1;32m\]&quot;</p>

<p class=MsoNoSpacing>RESET_COLOR=&quot;\[\033[0;0m\]&quot;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing>PS1=&quot;$RESET_COLOR\$(date +%H:%M) \w$YELLOW
\$(parse_git_branch)$LIGHT_GREEN\$ $RESET_COLOR&quot;</p>

<p class=MsoNoSpacing>&nbsp;</p>

<p class=MsoNoSpacing># Load virtualenvwrapper</p>

<p class=MsoNoSpacing>source virtualenvwrapper.sh &amp;&gt; /dev/null</p>

